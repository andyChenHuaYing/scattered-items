<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.alien.mybatis.samples.mapper.DepartmentMapper">
    <select id="selectDepartment" resultType="department">
        SELECT t1.deptno deptNo, t1.dname deptName, t1.loc deptLocation FROM dept t1
    </select>
</mapper>